<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @course.title %>
</p>

<p>
  <strong>Course number:</strong>
  <%= @course.course_number %>
</p>
<% if @course.students %>
	<% @course.students.each do |student| %>
		<li><%= student.name %>
		</li>
	<% end %>
<% end %>	
			

<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', courses_path %>
